    "dexie": "^4.0.0",
    "dexie-react-hooks": "^1.1.7",
    "lexical": "^0.22.0",
    "lucide-react": "^0.460.0",
    "openai": "^4.67.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.0",
    "uuid": "^11.0.0"
