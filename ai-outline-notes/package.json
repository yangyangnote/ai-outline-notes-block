  "type": "module",
  "private": true,
  "main": "dist-electron/main.js",
  "description": "AI 驱动的大纲笔记应用",
  "author": "AI Outline Notes Team",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
